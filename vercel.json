{
    "version": 2,
    "builds": [
        {
            "src": "./run.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/*.*",
            "dest": "/"
        }
    ],
    "functions": {
        "run.py": {
          "memory": 128,
          "maxDuration": 30
        }
    }
}